var marks = prompt(' Enter your marks');

if(marks>=80)
      console.log("A+");

else if(marks>=70)
     console.log("A");

else if(marks>=60)
     console.log("A-");

else if(marks>=50)
     console.log("B");

else if(marks>=40)
    console.log("c");

     else
    console.log("You Are Fail Bro");


    
